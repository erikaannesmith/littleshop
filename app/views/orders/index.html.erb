<h1 class= "cart_header">All Orders for <%= current_user.username %></h1>


<table>
  <tr>
    <th>Order</th>
    <th>User ID</th>
    <th>Submitted @</th>
  </tr>
  <% @orders.each do |order| %>
    <tr>
      <td><%= link_to "##{order.id}", user_order_path(current_user, order) %></td>
      <td><%= order.user_id %></td>
      <td><%= order.created_at %></td>
    </tr>
  <% end %>
</table>


